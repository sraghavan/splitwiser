# Database
# Local development (SQLite) - uncomment for local dev
# DATABASE_URL="file:./dev.db"

# Production (PostgreSQL) - get this from Supabase
DATABASE_URL="postgresql://postgres:[password]@[hostname]:5432/[database]?pgbouncer=true&connection_limit=1"

# Supabase URL and Keys (get these from Supabase dashboard)
NEXT_PUBLIC_SUPABASE_URL="your-supabase-url"
NEXT_PUBLIC_SUPABASE_ANON_KEY="your-supabase-anon-key"